<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="IS_MOUSE_CLICK_SELECTION_HONORS_CAMEL_WORDS" value="false" />
  </component>
  <component name="SpringEditorOptions">
    <option name="showProfilesPanel" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern icon="default" useCustomColors="true" case-sensitive="false" pattern="\btodo aellyli\b.*">
      <option name="FOREGROUND" value="ff0000" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <pattern icon="default" useCustomColors="true" case-sensitive="false" pattern="\bfixme aellyli\b.*">
      <option name="FOREGROUND" value="ff0000" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <filter name="Aelly TODO">
      <pattern index="2" />
      <pattern index="3" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="automaticallyInsertRequiredAttributes" value="false" />
    <option name="automaticallyInsertRequiredSubTags" value="false" />
    <option name="automaticallyStartAttribute" value="false" />
    <option name="insertQuotesForAttributeValue" value="false" />
  </component>
</application>